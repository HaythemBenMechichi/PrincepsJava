<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="211.0" prefWidth="360.0" stylesheets="@resources/styles/itemStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.workshopjdbc3a48.gui.EventItemController">
   <children>
      <VBox layoutY="-1.0" prefHeight="211.0" prefWidth="360.0" spacing="10.0" style="-fx-alignment: center;">
         <children>
            <ImageView fx:id="img" fitHeight="168.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="labelName">
               <font>
                  <Font name="Century Gothic Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="labelDate">
               <font>
                  <Font name="Century Gothic" size="16.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="event-item" />
      <String fx:value="shadow" />
   </styleClass>
</AnchorPane>
